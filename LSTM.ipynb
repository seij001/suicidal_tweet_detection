      "source": [
        "# Hyperparameters\n",
        "embedding_dim = 128\n",
        "hidden_dim = 256\n",
        "output_dim = 2  # suicidal/non-suicidal\n",
        "n_layers = 2\n",
        "bidirectional = True\n",
        "dropout = 0.3"
      ]
